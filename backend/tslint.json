{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "quotemark":false,
        "object-literal-sort-keys":false,
        "no-implicit-dependencies":false,
        "ordered-imports": false,
        "no-null-keyword": true
    },
    "rulesDirectory": []
  }